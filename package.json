{
  "name": "grown",
  "version": "0.22.0",
  "description": "Experimental DSL for web applications",
  "author": "Alvaro Cabrera",
  "license": "MIT",
  "main": "lib/index.js",
  "bin": "bin/grown",
  "scripts": {
    "dev:next": "jasmine-node _plugs/spec --coffee --verbose --autoTest --captureExceptions --watchFolders lib _plugs/conn _plugs/router _plugs/grown.js",
    "dev": "jasmine-node spec --coffee --verbose --autoTest --captureExceptions --watchFolders lib",
    "lint": "eslint bin lib spec examples",
    "spec": "jasmine-node spec --coffee --noStackTrace --captureExceptions",
    "cover": "istanbul cover --root lib --x '**/spec/**' -- jasmine-node --coffee spec",
    "cover:up": "codecov --file=coverage/lcov.info --disable=gcov -e TRAVIS_NODE_VERSION",
    "example": "node examples/_runner.js",
    "test": "npm run lint && npm run cover",
    "test:uws": "./examples/_test.sh"
  },
  "dependencies": {
    "accepts": "^1.3.4",
    "chalk": "^2.3.0",
    "clean-stack": "^1.3.0",
    "dotenv": "^4.0.0",
    "fast-equals": "^1.0.0",
    "fs-extra": "^4.0.2",
    "glob": "^7.1.2",
    "global-or-local": "^0.1.0",
    "haki": "^0.9.0",
    "log-pose": "^0.3.1",
    "mime": "^2.0.3",
    "object-new": "^0.1.2",
    "send": "^0.16.1",
    "source-map-support": "^0.5.0",
    "type-is": "^1.6.15",
    "wargs": "^0.7.2"
  },
  "devDependencies": {
    "body-parser": "^1.16.1",
    "codecov": "^3.0.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-alpha.2",
