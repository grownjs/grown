{
  "name": "homegrown",
  "version": "0.1.0",
  "description": "Homegrown nodejs web framework",
  "author": "Alvaro Cabrera",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "dev": "jasmine-node spec --coffee --verbose --autoTest --captureExceptions --watchFolders lib",
    "lint": "eslint bin lib spec",
    "spec": "jasmine-node spec --coffee --noStackTrace --captureExceptions",
    "cover": "istanbul cover --root lib --x '**/spec/**' -- jasmine-node --coffee spec",
    "cover:up": "codecov --file=coverage/lcov.info --disable=gcov -e TRAVIS_NODE_VERSION",
    "example": "node examples/_runner.js",
    "test": "yarn lint && yarn cover",
    "test:uws": "./examples/_test.sh",
    "build": "./examples/_uws.sh"
  },
  "dependencies": {
    "accepts": "^1.3.3",
    "clean-stack": "^1.1.1",
    "dotenv": "^4",
    "glob": "^7.0.6",
    "object-new": "https://github.com/pateketrueke/object-new.git#master",
    "type-is": "^1.6.14"
  },
  "devDependencies": {
    "body-parser": "^1.15.2",
    "codecov": "^1.0.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-alpha.2",
    "csurf": "^1.9.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-plugin-import": "^1.14.0",
    "formidable": "^1.0.17",
    "istanbul": "^0.4.3",
    "jasmine-node": "2.0.0-beta4",
    "json-schema-sequelizer": "https://github.com/pateketrueke/json-schema-sequelizer.git#master",
    "method-override": "^2.3.7",
    "mock-req": "^0.2.0",
    "mock-res": "^0.3.3",
    "morgan": "^1.7.0",
    "pg": "^6.1.2",
    "route-mappings": "0.3.2",
    "sequelize": "^3.27.0",
    "serve-index": "^1.8.0",
    "serve-static": "^1.11.1",
    "sqlite3": "^3.1.8",
    "std-mocks": "^1.0.1",
    "winston": "^2.3.0"
  }
}
