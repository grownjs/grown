{
  "name": "grown",
  "version": "0.4.4",
  "description": "Experimental DSL for web applications",
  "author": "Alvaro Cabrera",
  "license": "MIT",
  "main": "lib/index.js",
  "bin": "bin/grown",
  "scripts": {
    "dev": "jasmine-node spec --coffee --verbose --autoTest --captureExceptions --watchFolders lib",
    "lint": "eslint bin lib spec examples",
    "spec": "jasmine-node spec --coffee --noStackTrace --captureExceptions",
    "cover": "istanbul cover --root lib --x '**/spec/**' -- jasmine-node --coffee spec",
    "cover:up": "codecov --file=coverage/lcov.info --disable=gcov -e TRAVIS_NODE_VERSION",
    "example": "node examples/_runner.js",
    "test": "yarn lint && yarn cover",
    "test:uws": "./examples/_test.sh",
    "build": "./examples/_uws.sh"
  },
  "dependencies": {
    "accepts": "^1.3.3",
    "chalk": "^1.1.3",
    "clean-stack": "^1.1.1",
    "dotenv": "^4",
    "glob": "^7.0.6",
    "haki": "^0.5.1",
    "log-pose": "^0.2.2",
    "mime": "^1.3.6",
    "mock-req": "^0.2.0",
    "mock-res": "^0.4.1",
    "object-new": "^0.0.2",
    "send": "^0.15.3",
    "source-map-support": "^0.4.11",
    "type-is": "^1.6.14",
    "wargs": "^0.7.0"
  },
  "devDependencies": {
    "body-parser": "^1.16.1",
    "codecov": "^1.0.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-alpha.2",
    "csurf": "^1.9.0",
