    "csurf": "^1.9.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.2.0",
    "formidable": "^1.1.1",
    "istanbul": "^0.4.3",
    "jasmine-node": "2.0.0-beta4",
    "json-schema-sequelizer": "^0.10.1",
    "method-override": "^2.3.7",
    "mock-req": "^0.2.0",
    "mock-res": "^0.5.0",
    "morgan": "^1.8.1",
    "nodemailer": "^4.3.1",
    "route-mappings": "^0.5.3",
    "sequelize": "^4.2.0",
    "serve-index": "^1.8.0",
    "serve-static": "^1.13.1",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "sqlite3": "^3.1.13",
    "uws": "^8.14.1",
    "winston": "^2.3.0"
