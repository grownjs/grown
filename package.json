    "json-schema-sequelizer": "https://github.com/pateketrueke/json-schema-sequelizer.git#master",
    "method-override": "^2.3.7",
    "morgan": "^1.8.1",
    "nodemailer": "^2",
    "pg": "^6.1.5",
    "pg-native": "^1.10.1",
    "route-mappings": "0.3.2",
    "sequelize": "^3.27.0",
    "serve-index": "^1.8.0",
    "serve-static": "1.12.3",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "sqlite3": "^3.1.8",
    "std-mocks": "^1.0.1",
