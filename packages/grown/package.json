{
  "name": "@grown/grown",
  "version": "0.0.0",
  "description": "Experimental DSL for web applications",
  "author": "Alvaro Cabrera <pateketrueke@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "dev": "jasmine-node spec --coffee --verbose --autoTest --captureExceptions --watchFolders lib",
    "lint": "eslint lib spec",
    "spec": "jasmine-node spec --coffee --noStackTrace --captureExceptions",
    "cover": "istanbul cover --root lib --x '**/spec/**' -- jasmine-node --coffee spec",
    "cover:up": "codecov --file=coverage/lcov.info --disable=gcov -e TRAVIS_NODE_VERSION",
    "test": "npm run lint && npm run cover"
  },
  "devDependencies": {
    "@grown/test": "^0.0.0",
    "@grown/conn": "^0.0.0",
    "@grown/router": "^0.0.0",
    "@grown/render": "^0.0.0",
    "codecov": "^3.0.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^0.4.3",
    "jasmine-node": "2.0.0-beta4"
  },
  "dependencies": {
    "clean-stack": "^1.3.0",
    "debug": "^3.1.0",
    "object-new": "^0.1.10"
  }
}
