{
  "name": "grown",
  "version": "0.0.0",
  "description": "Experimental DSL for web applications",
  "author": "Alvaro Cabrera <pateketrueke@gmail.com>",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "jasmine-node spec --coffee --verbose --autoTest --captureExceptions --watchFolders src",
    "lint": "eslint bin lib spec examples",
    "spec": "jasmine-node spec --coffee --noStackTrace --captureExceptions",
    "cover": "istanbul cover --root lib --x '**/spec/**' -- jasmine-node --coffee spec",
    "cover:up": "codecov --file=coverage/lcov.info --disable=gcov -e TRAVIS_NODE_VERSION",
    "test": "npm run lint && npm run cover"
  },
  "devDependencies": {
    "codecov": "^3.0.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^0.4.3",
    "jasmine-node": "2.0.0-beta4",
    "rollup": "^0.51.2"
  },
  "dependencies": {
    "object-new": "^0.1.3"
  },
  "files": [
    "dist"
  ]
}
