{
  "name": "@grown/server",
  "version": "0.0.5",
  "author": "Alvaro Cabrera <pateketrueke@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "dev": "jasmine-node spec --coffee --verbose --autoTest --captureExceptions",
    "lint": "eslint index.js util spec",
    "spec": "jasmine-node spec --coffee --noStackTrace --captureExceptions",
    "cover": "istanbul cover --x '**/spec/**' -- jasmine-node --coffee spec",
    "cover:up": "codecov --file=coverage/lcov.info --disable=gcov -e TRAVIS_NODE_VERSION",
    "test": "npm run lint && npm run cover"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "debug": "^4.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "codecov": "^3.1.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0"
  }
}
